<Window x:Class="p4lab8.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:p4lab8"
        mc:Ignorable="d"
        Title="MainWindow" Height="488" Width="878">
    <Grid>
        <TextBox Text="{Binding RepeatedPassword}"  Width="120" Height="30" Margin="286,202,394,202"/>
        <TextBox Text="{Binding Password}"  Width="120" Height="30" RenderTransformOrigin="-0.292,-0.56" Margin="285,144,395,240"/>
        <TextBox Text="{Binding Login}"  Width="120" Height="30" Margin="285,89,395,295"/>
        <Label Content="Password Repeat" Width="120" Height="30" Margin="164,201,516,203"/>
        <Label Content="Password" Width="120" Height="30" Margin="164,154,516,250"/>
        <Label Content="Login" Width="120" Height="30" Margin="164,99,516,305"/>
        <CheckBox Content="Temrs" Width="120" Height="30" Margin="285,247,395,137" IsChecked="{Binding TermsAccepted}"/>
        <DatePicker Margin="285,272,395,112" Width="120" Height="30" SelectedDate="{Binding BirthDate}"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="324,352,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.782,-1.501" Width="112" Height="32" Click="Button_Click"/>

    </Grid>
</Window>
